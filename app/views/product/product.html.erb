<h2>Product</h2>

<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Price</th>
        <th>Qty</th>
    </tr>
    
    <% @products.each do |product| %>
        <tr>
            <td><%= product.title %></td>
            <td><%= product.description %></td>
            <td><%= product.price %></td>
            <td><%= product.stock_quantity %></td>
        </tr>
    <% end %>   
</table>